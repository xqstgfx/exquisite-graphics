{
  "name": "@exquisite-graphics/contracts",
  "version": "0.1.5",
  "scripts": {
    "build": "yarn build:contracts && yarn build:package",
    "prepublishOnly": "yarn build",
    "test": "hardhat test",
    "compile": "hardhat clean && hardhat compile",
    "typechain": "rm -rf ./typechain && typechain --show-stack-traces --target ethers-v5 --out-dir ./typechain 'artifacts/!(build-info)/**/!(*.dbg*)*.json'",
    "build:contracts": "yarn compile && yarn typechain",
    "build:package": "rm -rf ./dist && tsc && cp typechain/*.d.ts dist/typechain && cp -R addresses dist && cp -R artifacts/contracts dist/artifacts && cp -R contracts dist",
    "deploy-test": "ts-node scripts/test.ts",
    "deploy": "ts-node scripts/deployExquisiteGraphics.ts --chainId 1 && cp -R addresses dist",
    "deploy-vault": "ts-node scripts/deployVault.ts --chainId 1 && cp -R addresses dist",
    "deploy-rink": "ts-node scripts/deployExquisiteGraphics.ts --chainId 4 && cp -R addresses dist",
    "deploy-vault-rink": "ts-node scripts/deployVault.ts --chainId 4 && cp -R addresses dist",
    "deploy-goerli": "ts-node scripts/deployExquisiteGraphics.ts --chainId 5 && cp -R addresses dist",
    "deploy-vault-goerli": "ts-node scripts/deployVault.ts --chainId 5 && cp -R addresses dist",
    "deploy-gnosis": "ts-node scripts/deployExquisiteGraphics.ts --chainId 100 && cp -R addresses dist",
    "deploy-vault-gnosis": "ts-node scripts/deployVault.ts --chainId 100 && cp -R addresses dist",
    "deploy-mumbai": "ts-node scripts/deployExquisiteGraphics.ts --chainId 80001 && cp -R addresses dist",
    "deploy-vault-mumbai": "ts-node scripts/deployVault.ts --chainId 80001 && cp -R addresses dist",
    "deploy-polygon": "ts-node scripts/deployExquisiteGraphics.ts --chainId 137 && cp -R addresses dist",
    "deploy-vault-polygon": "ts-node scripts/deployVault.ts --chainId 137 && cp -R addresses dist"
  },
  "devDependencies": {
    "@divergencetech/ethier": "^0.10.0",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.2.0",
    "@types/chai": "^4.2.21",
    "@types/chroma-js": "^2.1.3",
    "@types/fs-extra": "^9.0.12",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.2",
    "@types/sharp": "^0.29.4",
    "chai": "^4.3.4",
    "chroma-js": "^2.4.2",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.0.0",
    "ethers": "^5.0.0",
    "fs-extra": "^10.0.0",
    "hardhat": "^2.5.0",
    "hardhat-change-network": "^0.0.7",
    "hardhat-gas-reporter": "^1.0.4",
    "sharp": "^0.29.3",
    "ts-node": "^10.1.0",
    "typechain": "^5.1.2",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@0xsequence/sstore2": "github:0xsequence/sstore2",
    "@openzeppelin/contracts": "^4.3.1",
    "@primitivefi/hardhat-marmite": "^0.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "1ce1f84ec89f6cbc1ab4ae3b0af26c1a678de310"
}
